algoritmo "area do triangulo"

var
base,altura,area: real

inicio
escreval ( "digite a base: " )
leia (base)
escreval ( "digite a altura: " )
leia (altura)


area <- (base * altura) / 2
escreval ("valor do base:  ",base)
escreval("valor da altura:  ",altura)
escreval ("valor area:  ",area)

fimalgoritmo
